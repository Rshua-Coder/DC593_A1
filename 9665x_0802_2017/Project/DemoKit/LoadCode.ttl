connect 'localhost:4444 /DS /T=1'

sendln 'halt'
wait '>'

sendln 'reset init'
wait '>'

send 'load_image '
send param2
send '/DemoKit_Data/'
send param3
sendln '/FW96650A.bin 0x80000000'
wait '>'
wait '>'

strcompare param4 'Resume'
if result=0 then
    sendln 'reg pc 0x80000400'
    wait '>'
    sendln 'resume'
    wait '>'
endif

sendln 'exit'

disconnect 0